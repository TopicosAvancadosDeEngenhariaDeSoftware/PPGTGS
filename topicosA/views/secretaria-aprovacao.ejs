<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <title>Teste Dashboard</title>

    <% include partes/scripts-base %>

</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <% include partes/header %>
        
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
          <section class="sidebar">
            <!-- Sidebar Menu -->
            <ul class="sidebar-menu" data-widget="tree">
              <!-- Optionally, you can add icons to the links -->
              <li class="active"><a href="#"><i class="fa fa-link"></i> <span>Link</span></a></li>
              <li><a href="#"><i class="fa fa-link"></i> <span>Another Link</span></a></li>
              <li class="treeview">
                <a href="#"><i class="fa fa-link"></i> <span>Multilevel</span>
                  <span class="pull-right-container">
                      <i class="fa fa-angle-left pull-right"></i>
                    </span>
                </a>
                <ul class="treeview-menu">
                  <li><a href="#">Link in level 2</a></li>
                  <li><a href="#">Link in level 2</a></li>
                </ul>
              </li>
            </ul>
            <!-- /.sidebar-menu -->
          </section>
          <!-- /.sidebar -->
        </aside>
      
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
          <!-- Content Header (Page header) -->
          <section class="content-header">
            <h1>
              Aprovação de Cadastro
              
            </h1>
            
          </section>
      
          <!-- Main content -->
          <section class="content container-fluid">
                <div class="box-body table-responsive no-padding ">
                    <table id="tabela_discentes" class="table table-hover">
                        <tbody>
                            <tr>
                                <th>Nome</th>
                                <th>Email</th>
                            </tr>
                            <% for(var i = 0 ; i< discentes.length ;i++){ %>
                             <tr>
                                 <td><%= discentes[i].nome + " " + discentes[i].sobrenome %></td>
                                 <td><%= discentes[i].email %></td>
                                 <td><a href= "<%='../discentes/alterar?id='+discentes[i].id_discente%>" class="glyphicon glyphicon-ok" TITLE="Aceitar cadastro Discente"></a></td>
                             </tr> <!-- todo redirect and show info before "aceite"-->
                             <% } %>
                         </tbody>
                 
                    </table>
                </div>
      
          </section>
          <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->       
        
      </div>
      </body>
      </html>